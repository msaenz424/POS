	<parameter name="my_date1" class="java.lang.String"/>
	<parameter name="my_date2" class="java.lang.String"/>
	<parameter name="my_top_number" class="java.lang.String"/>
	<queryString>
		<![CDATA[	SELECT p.productName, sum(od.quantity) as TotalQuantity, sum(od.quantity*od.unitPrice-od.discount) as Total
	FROM OrderDetails od 
	INNER JOIN Orders o ON od.orderID = o.orderID
	INNER JOIN Product p ON od.productID = p.productID
    WHERE o.orderDate >= $P!{my_date1} AND o.OrderDate <= $P!{my_date2}
	GROUP BY p.productName
	ORDER BY TotalQuantity DESC, Total DESC LIMIT $P!{my_top_number}]]>
	</queryString>
